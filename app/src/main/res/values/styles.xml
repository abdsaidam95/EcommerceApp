<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#5FAE4F</item>
        <item name="colorPrimaryDark">#5FAE4F</item>
        <item name="colorAccent">#5FAE4F</item>
<!--        <item name="android:statusBarColor">@android:color/transparent</item>-->
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="SpinnerAppTheme" parent="android:Widget.Spinner">
        <item name="android:background">@android:color/white</item>
    </style>
    <style name="SpinnerDropDownItemAppTheme" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/ic_launcher_background</item>
    </style>

</resources>